		"apollo-boost": "^0.4.7",
		"axios": "^0.19.2",
		"graphql": "^14.6.0",
		"isomorphic-unfetch": "^3.0.0",
		"moment": "^2.24.0",
		"next": "^9.2.2",
		"react": "^16.13.0",
		"react-dom": "^16.13.0",
		"react-hook-form": "^4.10.1",
		"react-markdown": "^4.2.2",
		"react-moment": "^0.9.7",
		"sass": "^1.26.2",
		"yup": "^0.28.1"
	},
	"devDependencies": {
		"@babel/core": "^7.8.6",
		"@babel/plugin-proposal-optional-chaining": "^7.7.5",
