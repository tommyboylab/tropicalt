{
  "name": "tropicalt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next -p 3000 --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "test:static": "npm run check-types && npm run lint && npm run prettier:validate",
    "types": "tsc --noEmit",
    "prettier": "prettier './components/**/*.+(ts|tsx|json|graphq|scss)' --write",
    "lint": "eslint './**/*.js' './**/*.ts' './**/*.tsx' --fix",
    "stylelint": "stylelint './**/*.scss' --fix",
    "fixall": "npm run prettier && npm run stylelint && npm run lint",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@app/gql": "file:./gql",
    "@hookform/resolvers": "^2.9.10",
    "deepmerge": "^4.2.2",
    "framer-motion": "^7.10.3",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.2",
    "isomorphic-unfetch": "^3.1.0",
    "lodash.isequal": "^4.5.0",
    "markdown-to-jsx": "^7.1.7",
    "moment": "^2.29.4",
    "next": "^13.0.3",
    "nookies": "^2.5.2",
    "preact": "^10.11.2",
    "preact-render-to-string": "^5.2.6",
    "react": "18.2.0",
    "react-check-auth": "^0.2.0-alpha.2",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.39.3",
    "react-moment": "^1.1.2",
    "redaxios": "^0.5.1",
    "sass": "^1.56.1",
    "sharp": "^0.31.2",
    "swr": "^1.3.0",
    "urql": "^3.0.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/gql-tag-operations-preset": "^1.7.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/lodash.isequal": "^4.5.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-perf": "^3.3.1",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.1",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.8.4"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  }
}
