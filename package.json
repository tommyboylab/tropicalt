		"axios": "^0.19.2",
		"graphql": "^15.0.0",
		"isomorphic-unfetch": "^3.0.0",
		"moment": "^2.24.0",
		"next": "^9.3.5",
		"next-with-apollo": "^5.0.1",
		"react": "^16.13.1",
		"react-apollo": "^3.1.5",
		"react-dom": "^16.13.1",
		"react-hook-form": "^5.4.2",
		"react-markdown": "^4.2.2",
		"react-moment": "^0.9.7",
