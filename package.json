{
	"name": "tropicalt",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next -p PORT --hostname 0.0.0.0",
		"build": "next build",
		"start": "next start",
		"export": "next export",
		"test:static": "npm run check-types && npm run lint && npm run prettier:validate",
		"types": "tsc --noEmit",
		"prettier": "prettier './components/**/*.+(ts|tsx|json|graphq|scss)' --write",
		"lint": "eslint './**/*.js' './**/*.ts' './**/*.tsx' --fix",
		"stylelint": "stylelint './**/*.scss' --fix",
		"fixall": "npm run prettier && npm run stylelint && npm run lint",
		"codegen": "graphql-codegen --config codegen.yml"
	},
	"dependencies": {
		"@app/gql": "file:./gql",
		"@hookform/resolvers": "^2.8.8",
		"@prefresh/next": "^1.5.2",
		"deepmerge": "^4.2.2",
		"graphql": "^16.4.0",
		"isomorphic-unfetch": "^3.1.0",
		"lodash.isequal": "^4.5.0",
		"moment": "^2.29.3",
		"next": "^12.1.5",
		"nookies": "^2.5.2",
		"preact": "^10.7.1",
		"preact-render-to-string": "^5.2.0",
		"react-check-auth": "^0.2.0-alpha.2",
		"react-hook-form": "^7.30.0",
		"react-markdown": "^8.0.3",
		"react-moment": "^1.1.2",
		"redaxios": "^0.5.0",
		"sass": "^1.51.0",
		"swr": "^1.3.0",
		"urql": "^2.2.0",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^2.6.2",
		"@graphql-codegen/gql-tag-operations-preset": "^1.3.0",
		"@graphql-typed-document-node/core": "^3.1.1",
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/git": "^10.0.1",
		"@types/graphql": "^14.5.0",
		"@types/lodash.isequal": "^4.5.6",
		"@types/react": "^18.0.3",
		"@types/react-dom": "^18.0.3",
		"@types/yup": "^0.29.13",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"dotenv": "^16.0.0",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.5.0",
		"eslint-plugin-react-perf": "^3.3.1",
		"prettier": "^2.6.2",
		"stylelint": "^14.8.1",
		"stylelint-config-recommended-scss": "^6.0.0",
		"stylelint-order": "^5.0.0",
		"stylelint-prettier": "^2.0.0",
		"stylelint-scss": "^4.2.0",
		"typescript": "^4.6.4"
	},
	"plugins": [
		"@semantic-release/commit-analyzer",
		"@semantic-release/release-notes-generator",
		"@semantic-release/changelog",
		"@semantic-release/github",
		"@semantic-release/npm",
		"@semantic-release/git"
	],
	"release": {
		"prepare": [
			"@semantic-release/changelog",
			"@semantic-release/npm",
			{
				"path": "@semantic-release/git",
				"assets": [
					"package.json",
					"package-lock.json",
					"CHANGELOG.md"
				],
				"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
			}
		]
	}
}
