{
	"name": "tropicalt",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next -p PORT --hostname 0.0.0.0",
		"build": "next build",
		"start": "next start",
		"export": "next export",
		"test:static": "npm run check-types && npm run lint && npm run prettier:validate",
		"types": "tsc --noEmit",
		"prettier": "prettier './components/**/*.+(ts|tsx|json|graphq|scss)' --write",
		"lint": "eslint './**/*.js' './**/*.ts' './**/*.tsx' --fix",
		"stylelint": "stylelint './**/*.scss' --fix",
		"fixall": "npm run prettier && npm run stylelint && npm run lint",
		"codegen": "graphql-codegen --config codegen.yml"
	},
	"dependencies": {
		"@apollo/client": "^3.1.3",
		"@apollo/react-hooks": "^4.0.0",
		"@apollo/react-ssr": "^4.0.0",
		"apollo-server-core": "^2.15.1",
		"graphql": "^15.2.0",
		"isomorphic-unfetch": "^3.0.0",
		"moment": "^2.27.0",
		"next": "^9.5.2",
		"nookies": "^2.4.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-hook-form": "^6.3.3",
		"react-markdown": "^4.2.2",
		"react-moment": "^0.9.7",
		"redaxios": "^0.3.0",
		"sass": "^1.26.9",
		"yup": "^0.29.3"
	},
	"devDependencies": {
		"@babel/core": "^7.11.1",
		"@babel/plugin-proposal-optional-chaining": "^7.11.0",
		"@babel/plugin-transform-react-jsx": "^7.10.4",
		"@babel/plugin-transform-typescript": "^7.11.0",
		"@babel/preset-typescript": "^7.10.4",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"@types/react": "^16.9.46",
		"@types/react-dom": "^16.9.8",
		"@types/yup": "^0.29.4",
		"@typescript-eslint/eslint-plugin": "^3.9.0",
		"@typescript-eslint/parser": "^3.9.0",
		"@typescript-eslint/typescript-estree": "^3.9.0",
		"babel-loader": "^8.1.0",
		"babel-plugin-import-graphql": "^2.7.0",
		"dotenv": "^8.2.0",
		"eslint": "^7.6.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-react": "^7.20.6",
		"eslint-plugin-react-hooks": "^4.0.5",
		"eslint-plugin-react-perf": "^3.2.4",
		"npm": "^6.14.5",
		"prettier": "^2.0.5",
		"stylelint": "^13.6.1",
		"stylelint-config-recommended-scss": "^4.2.0",
		"stylelint-order": "^4.1.0",
		"stylelint-prettier": "^1.1.2",
		"stylelint-scss": "^3.18.0",
		"typescript": "^3.9.6"
	},
	"plugins": [
		"@semantic-release/commit-analyzer",
		"@semantic-release/release-notes-generator",
		"@semantic-release/changelog",
		"@semantic-release/github",
		"@semantic-release/npm",
		"@semantic-release/git"
	],
	"release": {
		"prepare": [
			"@semantic-release/changelog",
			"@semantic-release/npm",
			{
				"path": "@semantic-release/git",
				"assets": [
					"package.json",
					"package-lock.json",
					"CHANGELOG.md"
				],
				"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
			}
		]
	}
}
