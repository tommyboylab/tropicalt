		"@babel/preset-typescript": "^7.9.0",
		"@types/react": "^16.9.34",
		"@types/react-dom": "^16.9.6",
		"@types/yup": "^0.26.36",
		"@typescript-eslint/eslint-plugin": "^2.28.0",
		"@typescript-eslint/parser": "^2.28.0",
		"@typescript-eslint/typescript-estree": "^2.28.0",
