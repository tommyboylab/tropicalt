		"eslint-plugin-react-perf": "^3.2.3",
		"npm": "^6.14.4",
		"prettier": "^2.0.4",
		"stylelint": "^13.3.2",
		"stylelint-config-recommended-scss": "^4.2.0",
		"stylelint-order": "^4.0.0",
		"stylelint-prettier": "^1.1.2",
