{
    "name": "tropicalt",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --hostname 0.0.0.0",
        "build": "next build",
        "start": "next start",
        "test": "jest",
        "test:static": "npm run check-types && npm run lint && npm run prettier:validate",
        "prettier": "prettier \"**/*.+(ts|tsx|json|graphql)\"",
        "prettier:format": "npm run prettier -- --write",
        "prettier:validate": "npm run prettier -- --list-different",
        "check-types": "tsc --noEmit",
        "lint": "eslint './**/*.ts' './**/*.tsx'",
        "lint:fix": "eslint './**/*.ts' './**/*.tsx' --fix",
        "stylelint": "stylelint './**/*.scss' './**/*.css'",
        "stylelint:fix": "stylelint './**/*.scss' './**/*.css' --fix"
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.3",
        "@apollo/react-ssr": "^3.1.3",
        "@zeit/next-sass": "^0.2.1-canary.4",
        "apollo-boost": "^0.4.4",
        "axios": "^0.19.0",
        "graphql": "^14.5.8",
        "isomorphic-unfetch": "^3.0.0",
        "moment": "^2.24.0",
        "next": "^9.1.5",
        "next-compose-plugins": "^2.2.0",
        "next-optimized-images": "^2.5.4",
        "node-sass": "^4.13.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-hook-form": "^3.29.4",
        "react-markdown": "^4.2.2",
        "react-moment": "^0.9.7",
